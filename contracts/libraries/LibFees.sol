pragma solidity ^0.8.24;

library LibFees {
    uint256 internal constant userfeesPercent = 9000;
    uint256 internal constant devfeesPercent = 1000;
    uint256 internal constant MaxPercent = 10000;

    // fees
    uint256 internal constant baseFeeCharacter = 0.00003 ether;
    uint256 internal constant baseFeeFlag = 0.01 ether;
    uint256 internal constant baseFeeBuilding = 0.00003 ether;
    uint256 internal constant baseFeeShip = 0.00006 ether;
    uint256 internal constant baseFeeOdyssey = 0.00006 ether;

    function calculfeesPercent(
        uint256 _amount
    ) internal pure returns (uint256 user, uint256 fees) {
        return (
            ((_amount * userfeesPercent) / MaxPercent),
            ((_amount * devfeesPercent) / MaxPercent)
        );
    }
}
