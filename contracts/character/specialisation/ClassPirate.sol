pragma solidity ^0.8.24;

import "@openzeppelin/contracts/utils/Strings.sol";
import {LibCharacters} from "../../libraries/LibCharacters.sol";

library ClassPirate {
    /**
     * @dev Returns the attribute values for the Corsair character class.
     * @return attributs The attribute values for the Corsair character class.
     */
    function _getAttribute()
        internal
        pure
        returns (LibCharacters.Attribut memory)
    {
        LibCharacters.Attribut memory attributs;
        attributs.Minboarding = 80;
        attributs.Maxboarding = 120 - (attributs.Minboarding - 1);
        attributs.Minsailing = 60;
        attributs.Maxsailing = 100 - (attributs.Minsailing - 1);
        attributs.Mincharisma = 30;
        attributs.Maxcharisma = 40 - (attributs.Mincharisma - 1);
        attributs.classId = LibCharacters.CharacterClass.PIRATE;
        return attributs;
    }
}
